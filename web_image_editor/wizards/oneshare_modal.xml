<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="oneshare_modal_view_form" model="ir.ui.view">
        <field name="name">Oneshare Modal Wizard</field>
        <field name="model">oneshare.modal</field>
        <field name="arch" type="xml">
            <form string="Oneshare Modal">
                <header/>
                <sheet>
                    <group name="image">
                        <field name="image" widget="oneshare_image_editor" nolabel="1"/>
                    </group>
                </sheet>
                <footer>
                    <button special="cancel" string="Cancel" class="btn-secondary"/>
                    <!--<button name="action_save" type="object" string="Save" class="oe_highlight pull-right"/>-->
                </footer>
            </form>
        </field>
    </record>
</odoo>